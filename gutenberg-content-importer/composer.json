{
    "name": "automattic/gutenberg-content-importer",
    "description": "Transform content from anywhere into perfect Gutenberg blocks with one click",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Automattic",
            "homepage": "https://automattic.com"
        }
    ],
    "keywords": [
        "wordpress",
        "gutenberg",
        "import",
        "medium",
        "notion",
        "content",
        "migration"
    ],
    "homepage": "https://github.com/automattic/gutenberg-content-importer",
    "support": {
        "issues": "https://github.com/automattic/gutenberg-content-importer/issues",
        "source": "https://github.com/automattic/gutenberg-content-importer"
    },
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "wp-coding-standards/wpcs": "^3.0",
        "dealerdirect/phpcodesniffer-composer-installer": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "GCI\\": "includes/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "phpcs": "phpcs --standard=WordPress .",
        "phpcbf": "phpcbf --standard=WordPress ."
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
} 